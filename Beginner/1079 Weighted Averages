#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    int N;
    cin >> N;

    for (int i = 0; i < N; ++i) {
        float a, b, c;
        cin >> a >> b >> c;
        
        float weightedAverage = (2 * a + 3 * b + 5 * c) / 10;
        cout << fixed << setprecision(1) << weightedAverage << endl;
    }

    return 0;
}
