#include <iostream>
using namespace std;

int main() {
    int t,num,c1=0,c2=0;
    cin>>t;
    while(t--){
        cin>>num;
        if(num>=10 && num<=20){
            c1++;
        }else{
            c2++;
        }
    }
    cout<<c1<< " in" <<endl;
    cout<<c2<< " out"<<endl;

    return 0;
}
